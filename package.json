{
  "name": "mcp-gcal",
  "version": "0.1.0",
  "description": "MCP server for interacting with Google Calendar (create, list, get, update, delete events)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:mcp": "node dist/index.js",
    "dev": "tsc --watch",
    "dev:server": "tsx watch src/server.ts",
    "clean": "rimraf dist",
    "get-token": "tsx scripts/get-refresh-token.ts"
  },
  "keywords": [
    "mcp",
    "google-calendar",
    "model-context-protocol",
    "ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "googleapis": "^144.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "mcp-gcal": "./dist/index.js"
  }
}

